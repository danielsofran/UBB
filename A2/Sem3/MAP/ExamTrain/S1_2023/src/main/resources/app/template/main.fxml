<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="463.0" prefWidth="812.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/19" fx:controller="app.template.controller.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="76.0" prefWidth="772.0">
         <children>
            <ComboBox fx:id="comboBox" layoutX="29.0" layoutY="26.0" onAction="#cmbItemChoosed" prefHeight="26.0" prefWidth="229.0" />
            <Button fx:id="btnSelect" layoutX="554.0" layoutY="26.0" mnemonicParsing="false" onAction="#btnClicked" prefHeight="26.0" prefWidth="140.0" text="Selecteaza Hotel" />
         </children>
      </AnchorPane>
      <TableView fx:id="table">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
   </children>
</VBox>
